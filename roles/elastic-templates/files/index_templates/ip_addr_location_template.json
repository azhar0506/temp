{
  "version": 1,
  "order":   0,
  "index_patterns": [
    "ip_addr_location"
  ],
  "settings": {
    "index": {
      "number_of_shards":   "1",
      "number_of_replicas": "1",
      "refresh_interval":   "10s",
			"merge": {
				"scheduler": {
					"max_thread_count": "1"
				}
			}
    }
  },
  "aliases": {},
  "mappings": {
    "properties": {
      "@timestamp": {
        "type": "date"
      },
      "@version": {
        "type": "text",
        "fields": {
          "keyword": {
            "ignore_above": 256,
            "type": "keyword"
          }
        }
      },
      "accounting_event_time": {
        "format": "yyyy/MM/dd HH:mm:ss||yyyy/MM/dd||epoch_millis",
        "index": true,
        "type": "date"
      },
      "acct_status_type": {
        "ignore_above": 20,
        "index": true,
        "type": "keyword",
        "doc_values": true
      },
      "calling_station_id": {
        "ignore_above": 60,
        "index": true,
        "type": "keyword",
        "doc_values": true
      },
      "geoip": {
        "dynamic": true,
        "properties": {
          "ip": {
            "type": "ip"
          },
          "latitude": {
            "type": "half_float"
          },
          "location": {
            "type": "geo_point"
          },
          "longitude": {
            "type": "half_float"
          }
        }
      },
      "ip_addr": {
        "ignore_above": 20,
        "index": true,
        "type": "keyword"
      },
      "ipv6_addr": {
        "ignore_above": 40,
        "index": true,
        "type": "keyword"
      },
      "nas_ip_address": {
        "ignore_above": 20,
        "index": true,
        "type": "keyword"
      },
      "nas_port_id": {
        "index": false,
        "type": "text"
      },
      "network_type": {
        "ignore_above": 10,
        "index": true,
        "type": "keyword",
        "doc_values": true
      },
      "server_host_name": {
        "ignore_above": 80,
        "index": true,
        "type": "keyword",
        "doc_values": true
      },
      "session_id": {
        "ignore_above": 80,
        "index": true,
        "type": "keyword",
        "doc_values": true
      },
      "type": {
        "ignore_above": 256,
        "type": "keyword"
      },
      "user_realm": {
        "index": true,
        "type": "keyword",
        "doc_values": true
      }
    }
  }
}
