{
	"version": 2,
	"order":   0,
	"index_patterns": [
		"session-data*"
	],
	"settings": {
		"index": {
			"number_of_shards":   "1",
			"number_of_replicas": "1",
			"refresh_interval":   "15s",
			"merge": {
				"scheduler": {
					"max_thread_count": "1"
				}
			}
		}
	},
	"aliases": {},
	"mappings": {
		"properties": {
			"@timestamp": {
				"type": "date"
			},
			"@version": {
				"type": "text",
				"fields": {
					"keyword": {
						"ignore_above": 256,
						"type":         "keyword"
					}
				}
			},
			"accounting_event_time": {
				"format": "yyyy/MM/dd HH:mm:ss||yyyy/MM/dd||epoch_millis",
				"index":  true,
				"type":   "date"
			},
			"acct_status_type": {
				"ignore_above": 10,
				"index":        true,
				"type":         "keyword",
				"doc_values":   true
			},
			"calling_station_id": {
				"ignore_above": 60,
				"index":        true,
				"type":         "keyword",
				"doc_values":   true
			},
			"cumulative_duration_secs": {
				"type": "long"
			},
			"ip_addr": {
				"ignore_above": 20,
				"index":        true,
				"type":         "keyword"
			},
			"ipv6_addr": {
				"ignore_above": 40,
				"index":        true,
				"type":         "keyword"
			},
			"nas_ip_address": {
				"ignore_above": 20,
				"index":        true,
				"type":         "keyword"
			},
			"nas_ip_location": {
				"type": "geo_point"
			},
			"nas_port": {
				"type": "long"
			},
			"nas_port_id": {
				"eager_global_ordinals":       false,
				"norms":                       false,
				"index":                       true,
				"store":                       false,
				"type":                        "keyword",
				"split_queries_on_whitespace": false,
				"index_options":               "docs",
				"doc_values":                  true
			},
			"network_type": {
				"ignore_above": 10,
				"index":        true,
				"type":         "keyword",
				"doc_values":   true
			},
			"radius_username": {
				"index":      true,
				"type":       "keyword",
				"doc_values": true
			},
			"server_host_name": {
				"ignore_above": 80,
				"index":        true,
				"type":         "keyword",
				"doc_values":   true
			},
			"session_id": {
				"ignore_above": 80,
				"index":        true,
				"type":         "keyword",
				"doc_values":   true
			},
			"type": {
				"ignore_above": 256,
				"type":         "keyword"
			},
			"user_realm": {
				"index":      true,
				"type":       "keyword",
				"doc_values": true
			}
		}
	}
}
