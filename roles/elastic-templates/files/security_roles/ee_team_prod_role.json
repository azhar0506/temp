{
	"cluster": [],
	"indices": [{
		"names": [
			"acct*"
		],
		"privileges": [
			"read"
		],
		"field_security": {
			"grant": [
				"accounting_event_time",
				"acct_status_type",
				"acct_terminate_cause",
				"calling_station_id",
				"cumulative_duration_secs",
				"input_gigawords",
				"input_octets",
				"input_packets",
				"ip_addr",
				"ipv6_addr",
				"nas_ip_address",
				"nas_port",
				"nas_port_type",
				"output_gigawords",
				"output_octets",
				"output_packets",
				"radius_username",
				"server_host_name",
				"session_id",
				"type",
				"user_realm"
			],
			"except": []
		},
		"query": {
			"bool": {
				"must": [{
					"match": {
						"type": "mna"
					}
				}, {
					"bool": {
						"should": [{
							"match": {
								"user_realm": "FS"
							}
						}, {
							"match": {
								"user_realm": "FS.NET"
							}
						}, {
							"match": {
								"user_realm": "BLACKFRIARS.TEST"
							}
						}, {
							"match": {
								"user_realm": "HARLEQUINS.TEST3"
							}
						}, {
							"match": {
								"user_realm": "HARLBUSINESS.COM"
							}
						}, {
							"match": {
								"user_realm": "BB8.FREESERVE.CO.UK"
							}
						}, {
							"match": {
								"user_realm": "BB4.FREESERVE.CO.UK"
							}
						}, {
							"match": {
								"user_realm": "BB3.FREESERVE.CO.UK"
							}
						}, {
							"match": {
								"user_realm": "BB2.FREESERVE.CO.UK"
							}
						}, {
							"match": {
								"user_realm": "BB1.FREESERVE.CO.UK"
							}
						}, {
							"match": {
								"user_realm": "BB.FREESERVE.CO.UK"
							}
						}],
						"minimum_should_match": 1
					}
				}]
			}
		},
		"allow_restricted_indices": false
	}],
	"applications": [],
	"run_as": [],
	"metadata": {
		"version":     2,
		"description": "Deployed with Ansible"
	},
	"transient_metadata": {
		"enabled": true
	}
}
