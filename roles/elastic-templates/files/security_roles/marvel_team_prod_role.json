{
	"cluster": [],
	"indices": [{
		"names": [
			"acct*"
		],
		"privileges": [
			"read"
		],
		"field_security": {
			"grant": [
				"accounting_event_time",
				"acct_interim_reason",
				"acct_status_type",
				"acct_terminate_cause",
				"calling_station_id",
				"cumulative_duration_secs",
				"input_gigawords",
				"input_octets",
				"input_packets",
				"ip_addr",
				"ip_addr_netmask",
				"ip_framed_route",
				"ipv6_addr",
				"nas_ip_address",
				"nas_port",
				"nas_port_id",
				"nas_port_type",
				"network_type",
				"openreach_id",
				"output_gigawords",
				"output_octets",
				"output_packets",
				"radius_username",
				"session_id",
				"type"
			],
			"except": []
		},
		"allow_restricted_indices": false
	},
	{
		"names": [
			"auth*"
		],
		"privileges": [
			"read"
		],
		"field_security": {
			"grant": [
				"base_user_name",
				"calling_station_id",
				"destination_address",
				"disposition_mode",
				"event_timestamp",
				"nas_id",
				"nas_ip_address",
				"nas_port_id",
				"radius_username",
				"service_type",
				"user_realm"
			],
			"except": []
		},
		"allow_restricted_indices": false
	}
	],
	"applications": [],
	"run_as": [],
	"metadata": {
		"version":     3,
		"description": "Deployed with Ansible"
	},
	"transient_metadata": {
		"enabled": true
	}
}
