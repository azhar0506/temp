{
	"cluster": [],
	"indices": [{
		"names": [
			"acct*"
		],
		"privileges": [
			"read"
		],
		"field_security": {
			"grant": [
				"@timestamp",
			  "accounting_event_time",
			  "acct_interim_reason",
			  "acct_status_type",
			  "acct_terminate_cause",
			  "calling_station_id",
			  "framed_pool",
			  "framed_protocol",
			  "ip_addr",
			  "ip_addr_netmask",
			  "ip_framed_route",
			  "ipv6_addr",
			  "nas_ip_address",
			  "nas_port",
			  "nas_port_id",
			  "nas_port_type",
			  "openreach_id",
			  "radius_username",
			  "server_host_name",
			  "session_id"
			],
			"except": []
		},
		"query": {
			"bool": {
				"must": [{
					"match": {
						"type": "mna"
					}
				}
				]
			}
		},
		"allow_restricted_indices": false
	},
	{
		"names": [
			"auth-mna*"
		],
		"privileges": [
			"read"
		],
		"field_security": {
			"grant": [
				"@timestamp",
			  "alc_tunnel_serv_id",
			  "base_user_name",
			  "bt_bb_special_service",
			  "calling_station_id",
			  "event_timestamp",
			  "indicator",
			  "nas_id",
			  "nas_ip_address",
			  "nas_port",
			  "nas_port_id",
			  "radius_username",
			  "server_host_name",
			  "tunnel_assignment_id",
			  "tunnel_client_auth_id",
			  "tunnel_client_endpoint",
			  "tunnel_server_endpoint"
			],
			"except": []
		},
		"allow_restricted_indices": false
	},
	{
		"names": [
			"wbc-rcd-filebeat*"
		],
		"privileges": [
			"read"
		],
		"field_security": {
			"grant": [
			 "@timestamp",
			 "message"
			],
			"except": []
		},
		"query": {
			"bool": {
				"must": [
					{
						"match": {
							"input.type": "container"
						}
					},
					{
						"bool": {
							"should": [{
									"match": {
										"kubernetes.container.name": "rcd-20c-api"
									}
								}, {
									"match": {
										"kubernetes.container.name": "rcd-20c-gui"
									}
								}, {
									"match": {
										"kubernetes.container.name": "rcd-21c-api"
									}
								}, {
									"match": {
										"kubernetes.container.name": "rcd-21c-gui"
									}
								}, {
									"match": {
										"kubernetes.container.name": "rcd-20c-api-plsql-packages"
									}
								}, {
									"match": {
										"kubernetes.container.name": "rcd-20c-api-table-migration"
									}
								}
							],
							"minimum_should_match": 1
					}
				}
				]
			}
		},
		"allow_restricted_indices": false
	}
	],
	"applications": [{
		"application": "kibana-.kibana",
		"privileges": [
			"feature_dashboard.all",
			"feature_dev_tools.read",
			"feature_discover.read",
			"feature_savedObjectsManagement.all",
			"feature_savedObjectsTagging.all",
			"feature_visualize.all"
		],
		"resources": [
			"space:external-view"
		]
	}],
	"run_as": [],
	"metadata": {
		"version":     1,
		"description": "Deployed with Ansible"
	},
	"transient_metadata": {
		"enabled": true
	}
}
