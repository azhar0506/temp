{
	"cluster": [],
	"indices": [{
		"names": [
			"acct-*"
		],
		"privileges": [
			"read"
		],
		"field_security": {
			"grant": [
				"accounting_event_time",
				"acct_status_type",
				"calling_station_id",
				"cumulative_duration_secs",
				"input_octets",
				"ip_addr",
				"ipv6_addr",
				"nas_ip_address",
				"nas_port_id",
				"output_octets",
				"radius_username",
				"session_id"
			],
			"except": [
				"_meta"
			]
		},
		"query": {
			"bool": {
				"must": [{
					"match": {
						"type": "wbc"
					}
				}]
			}
		},
		"allow_restricted_indices": false
	}],
	"applications": [],
	"run_as": [],
	"metadata": {
		"version":     2,
		"description": "Deployed with Ansible"
	},
	"transient_metadata": {
		"enabled": true
	}
}
